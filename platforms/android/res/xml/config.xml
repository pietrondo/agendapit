<?xml version='1.0' encoding='utf-8'?>
<widget id="Agenda.Pit.com" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>AgendaPit</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="pietro.capriata@gmail.com" href="http://www.pietrocapriata.cme">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
</widget>
